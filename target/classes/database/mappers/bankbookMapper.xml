<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.bh.b1.bankbook.BankBookDAO">
  	
  	<select id="getList" resultType="com.bh.b1.bankbook.BankBookDTO">
  		SELECT * FROM BANKBOOK
  	</select>
  	
  	<select id="getSelect" parameterType="com.bh.b1.bankbook.BankBookDTO" resultType="com.bh.b1.bankbook.BankBookDTO">
  		SELECT * FROM BANKBOOK WHERE BOOKNUMBER=#{bookNumber}
  	</select>
  	
  	<insert id="setInsert" parameterType="com.bh.b1.bankbook.BankBookDTO">
  		INSERT INTO BANKBOOK VALUES(BANKBOOK_SEQ.NEXTVAL, #{bookName}, #{bookRate}, #{bookUse})
  	</insert>
  	
  </mapper>